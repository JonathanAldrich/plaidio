package plaidio.filesystem.validity.valid.parentalness._debuger;

import plaidio.filesystem.validity.valid.parentalness.Parented;
import plaidio.filesystem.validity.valid.parentalness.Parentless;

method immutable Boolean isParented(object) { match (object) { case Parented { true; } default { false; } }; }
method immutable Boolean isParentless(object) { match (object) { case Parentless { true; } default { false; } }; }

method immutable Boolean debugIsParented(object, immutable Boolean verbose) { ifElse (isParented(object)) { if (verbose) { printLine("The object is Parented"); }; true; } /*else*/ { if (verbose) { printLine("The object is NOT Parented"); }; false; }; }
method immutable Boolean debugIsParentless(object, immutable Boolean verbose) { ifElse (isParentless(object)) { if (verbose) { printLine("The object is Parentless"); }; true; } /*else*/ { if (verbose) { printLine("The object is NOT Parentless"); }; false; }; }
