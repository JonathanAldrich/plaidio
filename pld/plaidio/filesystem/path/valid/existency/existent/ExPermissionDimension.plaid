import plaidio.filesystem.path.valid.existency.existent.expermission.ExecutabilityDimension;
import plaidio.filesystem.path.valid.existency.existent.expermission.ReadabilityDimension;
import plaidio.filesystem.path.valid.existency.existent.expermission.WritabilityDimension;

//------------------------------------------------------------------------------
/**
	CID:	TS-112111
	DESC:	"ExPermissionDimension" state region of "Existent" typestate.
**/
state ExPermissionDimension = ExecutabilityDimension with ReadabilityDimension with WritabilityDimension with
{
	//-----------------------------------------------------------------------------
	/**
		CID:	ST-1121111
		DESC:	Checks all dimensions of the object.
	**/
	method void initializeExPermissionDimension() /*[shared ExPermissionDimension >> shared ExPermissionDimension this]*/
	{
		initializeExecutabilityDimension();
		initializeReadabilityDimension();
		initializeWritabilityDimension();
	}
	//method void initializeExPermissionDimension() /*[shared ExPermissionDimension >> shared ExPermissionDimension this]*/
}